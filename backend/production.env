# Configurações para produção no Railway
PORT=3000
NODE_ENV=production

# MongoDB Atlas (substitua pelas suas credenciais)
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/database?retryWrites=true&w=majority

# JWT (use uma chave segura em produção)
JWT_SECRET=your-super-secret-jwt-key-here

# CORS (substitua pelo seu domínio do Railway)
CORS_ORIGIN=https://your-domain.railway.app

# Railway
RAILWAY_STATIC_URL=https://your-domain.railway.app
